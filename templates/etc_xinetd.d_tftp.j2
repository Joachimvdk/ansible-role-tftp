# TFTP configuration
#
# {{ ansible_managed }}
service tftp
{
  socket_type  = dgram
  protocol     = udp
  wait         = yes
  user         = root
  server       = /usr/sbin/in.tftpd
  server_args  = {{ tftp_server_args }}
  disable      = {{ tftp_disable }}
  per_source   = 11
  cps          = 100 2
  flags        = IPv4
}
